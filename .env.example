# Environment Variables for Inbox Pro AI Landing Page
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:9000

# Gemini API Key (Get from https://makersuite.google.com/app/apikey)
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Gmail OAuth Credentials (Get from Google Cloud Console)
NEXT_PUBLIC_GMAIL_CLIENT_ID=your_gmail_client_id_here
NEXT_PUBLIC_GMAIL_CLIENT_SECRET=your_gmail_client_secret_here
NEXT_PUBLIC_GMAIL_PROJECT_ID=your_project_id_here

# Route URLs (optional - defaults to localhost ports)
NEXT_PUBLIC_SETUP_URL=http://localhost:3000
NEXT_PUBLIC_EMAIL_ANALYSIS_URL=http://localhost:3002
NEXT_PUBLIC_SMART_REPLY_URL=http://localhost:3003
NEXT_PUBLIC_FOLLOW_UP_URL=http://localhost:3004
NEXT_PUBLIC_FOLLOW_UP_ANALYTICS_URL=http://localhost:3005
NEXT_PUBLIC_MEETING_INTENT_URL=http://localhost:3006
NEXT_PUBLIC_PRIORITY_FEEDBACK_URL=http://localhost:3007
NEXT_PUBLIC_SMART_QUICK_REPLY_URL=http://localhost:3008
NEXT_PUBLIC_FEEDBACK_FOLLOW_UP_URL=http://localhost:3009
